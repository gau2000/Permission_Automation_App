pipeline {    agent any    environment {        SONAR_SCANNER_HOME = tool 'SonarScanner'    }    stages {        stage('Checkout Code') {            steps {                git branch: 'main', credentialsId: 'your-credentials-id', url: 'https://github.com/gau2000/Permission_Automation_App.git'            }        }        stage('Install Dependencies') {            steps {                bat 'npm install'            }        }        stage('Lint Check') {            steps {                bat 'npx eslint src/**/*.tsx src/**/*.ts'            }        }        stage('SonarQube Analysis') {            steps {                withSonarQubeEnv('SonarQube') {                    bat "${SONAR_SCANNER_HOME}\bin\sonar-scanner -Dsonar.projectKey=pexels-app -Dsonar.sources=src -Dsonar.host.url=http://localhost:9000 -Dsonar.token=your_token"                }            }        }        stage('Build React App') {            steps {                bat 'npm run build'            }        }    }}
